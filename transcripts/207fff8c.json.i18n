         "endTime": 101705,
         "startTime": 98700
      },
      {
         "text": {
            "en": "not the center.",
            "pl": "Nie w centrum.",
            "ar": "ليس المركز."
         },
         "endTime": 103201,
         "startTime": 101705
      },
      {
         "text": {
